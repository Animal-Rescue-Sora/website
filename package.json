{
  "name": "animal_rescue_sora",
  "version": "1.0.0",
  "description": "Animal Rescue SORA website.",
  "homepage": "https://animal-rescue-sora.netlify.app/",
  "private": true,
  "main": "./src/index.js",
  "engines": {
    "node": "14.18.1",
    "npm": "6.14.15"
  },
  "_id": "npm@6.14.15",
  "_npmVersion": "6.14.15",
  "_nodeVersion": "14.18.1",
  "browserslist": [
    "edge 91",
    "chrome 91",
    "ios_saf 14",
    "safari 14",
    "firefox 90"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage",
          "corejs": 3
        }
      ]
    ]
  },
  "eslintConfig": {
    "root": true,
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "env": {
      "browser": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint-config-google"
    ],
    "plugins": [
      "vue"
    ],
    "rules": {
      "generator-star-spacing": "off",
      "max-len": "off"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "grid": true,
        "flexbox": "no-2009"
      },
      "css-mqpacker": {
        "sort": true
      },
      "postcss-mq-last": {},
      "postcss-sorting": {},
      "postcss-flexbugs-fixes": {},
      "postcss-discard-comments": {}
    }
  },
  "stylelint": {
    "extends": "stylelint-config-recommended-scss",
    "plugins": [],
    "rules": {
      "no-descending-specificity": null,
      "selector-pseudo-element-no-unknown": [
        true,
        {
          "ignorePseudoElements": [
            "v-deep"
          ]
        }
      ]
    },
    "defaultSeverity": "warning"
  },
  "scripts": {
    "🌊 環境構築コマンド": "",
    "init:setup": "npm ci",
    "🌊 PurgeCSS で消しきれない Vuetify の Style を消去": "",
    "init:vuetify": "node vuetify.style.remover.js",
    "🌊 Node-Sass のエラーが出ないように DartSass へ変換": "",
    "init:dartSass": "npx sass-migrator division ./node_modules/vuetify/src/**/*.sass && npx sass-migrator division ./node_modules/vuetify/src/**/*.scss && npx sass-migrator division ./node_modules/vuetify/src/**/**/*.sass && npx sass-migrator division ./node_modules/vuetify/src/**/**/*.scss",
    "init": "run-s init:*",
    "👹 Webpack から `./dist/docs/common/**/*.{css, js}` を出力": "",
    "build:webpack": "webpack --config webpack.prod.js",
    "👹 Pug から `./dist/docs/__template/**/*.html` を出力": "",
    "build:pug": "pug src/pug --hierarchy -o dist -b src/pug -P --doctype html",
    "👹 Pug から出力される不要な html ファイルを消去": "",
    "build:clean": "rimraf 'dist/includes/**/*.html'",
    "👹 `./dist/` 配下の空のディレクトリを消去": "",
    "build:empty": "delete-empty dist",
    "build:optim": "node css.optimizer",
    "👹 ↑の build:* コマンドを直列で実行": "",
    "build": "run-s build:*",
    "release:clean": "rimraf 'docs/*'",
    "release:copy": "ncp ./dist/docs/ ./docs/",
    "release:minify": "html-minifier --file-ext html --input-dir docs/ --output-dir docs/ -c html.minifier.config.js",
    "release": "run-s release:*",
    "👺 Pug から `./dist/docs/__template/**/*.html` を出力": "",
    "start:pug": "pug src/pug --hierarchy -o dist -b src/pug -P --doctype html -w",
    "👺 Webpack を起動": "",
    "start:webpack": "webpack --watch --config webpack.dev.js",
    "👺 ↑の start:* コマンドを並列で実行": "",
    "start": "run-p start:*",
    "bs": "browser-sync start --config bs.config.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "@ossan_meowwwing",
  "license": "ISC",
  "contributors": [
    {
      "name": "@ossan_meowwwing",
      "email": "ossan.meowwwing@icloud.com"
    }
  ],
  "maintainers": [
    {}
  ],
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@vue/compiler-sfc": "^3.2.21",
    "autoprefixer": "^10.4.0",
    "axios": "^0.24.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "babel-preset-vue": "^2.0.2",
    "browser-sync": "^2.27.7",
    "browser-sync-webpack-plugin": "^2.3.0",
    "compression-webpack-plugin": "^9.0.0",
    "core-js": "^3.19.1",
    "css-loader": "^6.5.0",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "css-mqpacker": "^7.0.0",
    "dayjs": "^1.10.7",
    "delete-empty": "^3.0.0",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-vue": "^7.20.0",
    "eslint-webpack-plugin": "^3.1.0",
    "fibers": "^5.0.0",
    "file-loader": "^6.2.0",
    "fs": "0.0.1-security",
    "get-youtube-id": "^1.0.1",
    "glob-all": "^3.2.1",
    "html-minifier": "^4.0.0",
    "mini-css-extract-plugin": "^2.4.3",
    "modern-css-reset": "^1.4.0",
    "ncp": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "postcss": "^8.3.11",
    "postcss-discard-comments": "^5.0.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-load-config": "^3.1.0",
    "postcss-loader": "^6.2.0",
    "postcss-mq-last": "^1.0.2",
    "postcss-sorting": "^7.0.1",
    "pug": "^3.0.2",
    "pug-cli": "^1.0.0-alpha6",
    "purgecss-webpack-plugin": "^4.0.3",
    "rimraf": "^3.0.2",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "sass-migrator": "^1.5.2",
    "sass-resources-loader": "^2.2.4",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-scss": "^3.21.0",
    "stylelint-webpack-plugin": "^3.1.0",
    "swiper": "^7.2.0",
    "v-scroll-lock": "^1.3.1",
    "vue": "^2.6.14",
    "vue-axios": "^3.3.7",
    "vue-clickaway": "^2.2.2",
    "vue-fragment": "1.5.1",
    "vue-loader": "^15.9.8",
    "vue-mq": "^1.0.1",
    "vue-slide-up-down": "^2.0.1",
    "vue-template-compiler": "^2.6.14",
    "vuetify": "^2.5.10",
    "vuex": "^3.6.2",
    "webpack": "^5.60.0",
    "webpack-cli": "^4.9.1",
    "webpack-manifest-plugin": "^4.0.2",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {}
}
